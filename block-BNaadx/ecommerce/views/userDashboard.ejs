<%- include('partials/userDashboardHeader') -%>
    <main>
        <div class="hero-user-dashboard">
            <div class="container ">
                <div class="user-dashboard">
                    <%products.forEach(product => { %>
                        <div class="product">
                            <div class="product_image">
                                <img class= "img" src="/uploads/<%= product.avatar_of_product%>" alt="">
    
                            </div>
                            <ul class="product_content">
                                <li class="product_name">
                                    <%= product.name_of_product%>
                                </li>
                                <li>
                                    MRP : INR<%= product.price_of_product%>
                                </li>
                                <li>
                                    <a href="/users/<%= product.id%>">
                                        <button>Learn More...</button>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    <% })%>
                </div>
                
            </div>
    </main>